{
    "name": "mosaic-data-table",
    "private": true,
    "version": "0.0.0",
    "scripts": {
        "clean:dist": "rimraf dist",
        "ng": "ng",
        "test:lib": "ng test mosaic-data-table --watch=true",
        "test:lib:ci": "ng test mosaic-data-table --watch=false",
        "lint:lib:ts": "ng lint mosaic-data-table",
        "build:lib:scss": "node-sass projects/mosaic-data-table/src/styles/themes/ -o dist/mosaic-data-table/prebuilt-themes/ --importer ./node_modules/node-sass-tilde-importer --source-map true --source-map-contents true",
        "build:lib:scss:copy": "cpx \"./projects/mosaic-data-table/src/styles/**/*\"  \"./dist/mosaic-data-table\"",
        "build:lib:dev": "ng build mosaic-data-table && npm run build:lib:scss && npm run build:lib:scss:copy",
        "build:lib:prod": "ng build mosaic-data-table --prod && npm run build:lib:scss && npm run build:lib:scss:copy",
        "start": "npm run clean:dist && npm run build:lib:dev && npm run start:demo",
        "start:demo": "ng serve mosaic-data-table-demo --open --port 4201"
    },
    "dependencies": {
        "@angular/animations": "^8.0.0",
        "@angular/common": "^8.0.0",
        "@angular/compiler": "^8.0.0",
        "@angular/core": "^8.0.0",
        "@angular/forms": "^8.0.0",
        "@ptsecurity/cdk": "^8.0.0-beta.3",
        "@ptsecurity/mosaic": "^8.0.0-beta.3",
        "@ptsecurity/mosaic-icons": "~2.10.0",
        "angular-resizable-element": "^3.2.4",
        "core-js": "^2.6.4",
        "lodash": "^4.17.11",
        "moment": "^2.24.0",
        "rxjs": "^6.4.0",
        "tslib": "^1.9.3",
        "zone.js": "^0.9.1"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "^0.800.2",
        "@angular-devkit/build-ng-packagr": "^0.800.2",
        "@angular/cli": "^8.0.0",
        "@angular/compiler-cli": "^8.0.0",
        "@angular/language-service": "^8.0.0",
        "@angular/platform-browser": "^8.0.0",
        "@angular/platform-browser-dynamic": "^8.0.0",
        "@angular/platform-server": "^8.0.0",
        "@angular/router": "^8.0.0",
        "@types/node": "^10.9.4",
        "cpx": "^1.5.0",
        "jasmine-core": "^3.3.0",
        "jasmine-spec-reporter": "~4.2.1",
        "karma": "^4.0.0",
        "karma-chrome-launcher": "^2.2.0",
        "karma-coverage-istanbul-reporter": "^2.0.4",
        "karma-jasmine": "^2.0.1",
        "karma-jasmine-html-reporter": "^1.4.0",
        "ng-packagr": "^4.7.1",
        "node-sass-tilde-importer": "^1.0.2",
        "rimraf": "^2.6.3",
        "ts-loader": "^5.3.3",
        "ts-node": "^8.0.2",
        "tsickle": "^0.34.3",
        "tslint": "^5.12.1",
        "typescript": "~3.4.x",
        "webpack": "^4.29.3",
        "webpack-cli": "^3.2.3"
    }
}
